<ul>
  <% if ::Refinery::Feedbacks::Admin::FeedbacksController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.feedbacks_admin_feedbacks_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_feedbacks_admin_feedback_path,
                 :class => "add_icon" %>
  </li>
    <li>
      <%= link_to t('.edit_confirmation_email'),
                  refinery.feedbacks_admin_setting_path(:feedback_confirmation, :dialog => true),
                  :class => "edit_email_icon" %>
    </li>
<% if !searching? && ::Refinery::Feedbacks::Admin::FeedbacksController.sortable? && ::Refinery::Feedbacks::Feedback.many? %>
  <li>
    <%= link_to t('.reorder', :what => "Feedbacks"),
                 refinery.feedbacks_admin_feedbacks_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Feedbacks"),
                 refinery.feedbacks_admin_feedbacks_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>

<% end %>
</ul>
