<%= form_for [refinery, :feedbacks_admin, @setting] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @setting,
              :include_object_name => true %>


  <div class='field'>
    <%= f.label :question -%>
    <%= f.text_field :question -%>
  </div>

  <div class='field'>
    <%= f.label :mail_otd -%>
    <%= f.text_field :mail_otd -%>
  </div>


  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.settings.admin.settings.setting'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @setting.mail_adm) -%>
<% end -%>
